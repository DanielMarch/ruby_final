.col-md-4.column
.col-md-4.column
  %h1 Sign In
  = simple_form_for @user, url: user_session_path, method: :post, wrapper: :vertical_form do |f|
    = f.input :email, placeholder: 'Email Address'
    = f.input :password, placeholder: 'Password'
    %button.btn.btn-default{:type => "submit"} Sign In
.col-md-4.column
